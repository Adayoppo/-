/**app.wxss**/
.container {
    height: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: space-between;
    padding: 200rpx 0;
    box-sizing: border-box;
}

/*字体图标*/
@font-face {
    font-family: "iconfont";
    src: url('//at.alicdn.com/t/font_1331525_qosc23pxy5g.eot?t=1565188624908'); /* IE9 */
    src: url('//at.alicdn.com/t/font_1331525_qosc23pxy5g.eot?t=1565188624908#iefix') format('embedded-opentype'), /* IE6-IE8 */url('data:application/x-font-woff2;charset=utf-8;base64,') format('woff2'),
    url('//at.alicdn.com/t/font_1331525_qosc23pxy5g.woff?t=1565188624908') format('woff'),
    url('//at.alicdn.com/t/font_1331525_qosc23pxy5g.ttf?t=1565188624908') format('truetype'), /* chrome, firefox, opera, Safari, Android, iOS 4.2+ */url('//at.alicdn.com/t/font_1331525_qosc23pxy5g.svg?t=1565188624908#iconfont') format('svg'); /* iOS 4.1- */
}

.iconfont {
    font-family: "iconfont"!important;
    font-size: 16px;
    font-style: normal;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
}

